innerM = 17;
innerN = 17;

m = innerM + 2;
n = innerN + 2;

stepX = 1/(m-1);
stepY = 1/(n-1);
x = 0:stepX:1;
y = 0:stepY:1;
[X,Y] = meshgrid(x);
f = sin(X.^2 + Y.^2);
% 0 -boundary
f(1,:) = 0;
f(:, 1) = 0;
f(m, :) = 0;
f(:,n) = 0;

A = zeros(m*n, m*n);
f_flat = reshape
for i = 2:m-1
    for j = 2:n-1
        
    end
end
save('Input.mat', 'f','-v4');